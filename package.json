{
  "name": "api_kalygo2",
  "version": "2.1.0",
  "description": "Kalygo API 2.0",
  "main": "dist/index.js",
  "scripts": {
    "test": "jest --config ./jest.config.ts",
    "build": "tsc && tsc-alias",
    "start": "node dist/index.js",
    "ts-watch": "tsc -w",
    "watch": "nodemon --watch '**/*' --ignore 'node_modules/' -e 'ts' --exec 'ts-node --files -r tsconfig-paths/register' index.ts",
    "dev": "concurrently \"npm:ts-watch\" \"npm:watch\"",
    "stripe:getPlans": "ts-node -r tsconfig-paths/register --project tsconfig.json scripts/getPlans.ts",
    "stripe:deletePlan": "ts-node -r tsconfig-paths/register --project tsconfig.json scripts/deletePlan.ts",
    "stripe:getSubscriptions": "ts-node -r tsconfig-paths/register --project tsconfig.json scripts/getSubscriptions.ts",
    "stripe:createProduct": "ts-node -r tsconfig-paths/register --project tsconfig.json scripts/createProduct.ts",
    "stripe:createPrice": "ts-node -r tsconfig-paths/register --project tsconfig.json scripts/createPrice.ts",
    "stripe:createSubscription": "ts-node -r tsconfig-paths/register --project tsconfig.json scripts/createSubscription.ts",
    "jobProcessor": "nodemon --watch './jobProcessor' --watch 'jobHandlers' --exec 'ts-node -r tsconfig-paths/register' jobProcessor.ts",
    "credits:giveSummaryCredits": "ts-node -r tsconfig-paths/register scripts/giveSummaryCredits.ts",
    "credits:giveVectorSearchCredits": "ts-node -r tsconfig-paths/register scripts/giveVectorSearchCredits.ts",
    "generateTranslations": "node scripts/generateTranslations"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Kalygo-io/api_kalygo2.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Kalygo-io/api_kalygo2/issues"
  },
  "homepage": "https://github.com/Kalygo-io/api_kalygo2#readme",
  "_moduleAliases": {
    "@": ".",
    "@db": "./dist/db",
    "@utils": "./dist/utils",
    "@emails": "./dist/emails",
    "@middleware": "./dist/middleware",
    "@config": "./dist/config"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.338.0",
    "@aws-sdk/client-ses": "^3.332.0",
    "@dqbd/tiktoken": "^1.0.7",
    "@google-cloud/translate": "^7.2.2",
    "@prisma/client": "^4.14.0",
    "@types/express-serve-static-core": "^4.17.35",
    "argon2": "^0.30.3",
    "bull": "^4.10.4",
    "chromadb": "^1.5.2",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-winston": "^4.2.0",
    "faiss-node": "^0.2.1",
    "i18next": "^22.4.15",
    "i18next-fs-backend": "^2.1.1",
    "i18next-http-backend": "^2.2.0",
    "i18next-http-middleware": "^3.3.0",
    "jsonwebtoken": "^9.0.0",
    "langchain": "^0.0.84",
    "lodash.get": "^4.4.2",
    "lodash.pick": "^4.4.0",
    "module-alias": "^2.2.2",
    "multer": "^1.4.5-lts.1",
    "multer-s3": "^3.0.1",
    "nanoid": "^3.3.6",
    "openai": "^3.2.1",
    "pdf2json": "^3.0.4",
    "pdfjs-dist": "^3.8.162",
    "pickleparser": "^0.1.0",
    "pug": "^3.0.2",
    "stripe": "^12.7.0",
    "tsc-alias": "^1.8.6",
    "uuid": "^9.0.0",
    "winston": "^3.8.2",
    "winston-daily-rotate-file": "^4.7.1"
  },
  "devDependencies": {
    "@types/bull": "^4.10.0",
    "@types/cookie-parser": "^1.4.3",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/lodash.get": "^4.4.7",
    "@types/lodash.pick": "^4.4.7",
    "@types/module-alias": "^2.0.1",
    "@types/multer": "^1.4.7",
    "@types/multer-s3": "^3.0.0",
    "@types/pug": "^2.0.6",
    "@types/uuid": "^9.0.1",
    "concurrently": "^8.0.1",
    "nodemon": "^2.0.22",
    "prisma": "^4.14.0",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0"
  }
}
